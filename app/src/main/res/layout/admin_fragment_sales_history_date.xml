<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="8dp"
    android:paddingBottom="8dp">

    <EditText
        android:id="@+id/admin_sales_list_et_mm_from"
        android:layout_width="@dimen/admin_fragment_sales_history_et_mmdd_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:gravity="center"
        android:hint="MM"
        android:inputType="numberDecimal"
        android:maxLength="2"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <EditText
        android:id="@+id/admin_sales_list_et_mm_to"
        android:layout_width="@dimen/admin_fragment_sales_history_et_mmdd_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:layout_marginStart="12dp"
        android:gravity="center"
        android:hint="MM"
        android:inputType="numberDecimal"
        android:maxLength="2"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <EditText
        android:id="@+id/admin_sales_list_et_dd_to"
        android:layout_width="@dimen/admin_fragment_sales_history_et_mmdd_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:gravity="center"
        android:hint="DD"
        android:inputType="numberDecimal"
        android:maxLength="2"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <EditText
        android:id="@+id/admin_sales_list_et_yyyy_to"
        android:layout_width="@dimen/admin_fragment_sales_history_et_yyyy_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:gravity="center"
        android:hint="YYYY"
        android:inputType="numberDecimal"
        android:maxLength="4"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <EditText
        android:id="@+id/admin_sales_list_et_dd_from"
        android:layout_width="@dimen/admin_fragment_sales_history_et_mmdd_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:gravity="center"
        android:hint="DD"
        android:inputType="numberDecimal"
        android:maxLength="2"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <EditText
        android:id="@+id/admin_sales_list_et_yyyy_from"
        android:layout_width="@dimen/admin_fragment_sales_history_et_yyyy_width"
        android:layout_height="@dimen/admin_fragment_sales_history_et_height"
        android:layout_marginEnd="12dp"
        android:gravity="center"
        android:hint="YYYY"
        android:inputType="numberDecimal"
        android:maxLength="4"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.473" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="@dimen/admin_fragment_sales_history_slash_width"
        android:layout_height="@dimen/admin_fragment_sales_history_slash_height"
        android:text="/"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/admin_sales_list_et_dd_from"
        app:layout_constraintEnd_toStartOf="@+id/admin_sales_list_et_dd_from"
        app:layout_constraintTop_toTopOf="@+id/admin_sales_list_et_dd_from" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="@dimen/admin_fragment_sales_history_slash_width"
        android:layout_height="@dimen/admin_fragment_sales_history_slash_height"
        android:text="/"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/admin_sales_list_et_mm_to"
        app:layout_constraintStart_toEndOf="@+id/admin_sales_list_et_mm_to"
        app:layout_constraintTop_toTopOf="@+id/admin_sales_list_et_mm_to" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="@dimen/admin_fragment_sales_history_slash_width"
        android:layout_height="@dimen/admin_fragment_sales_history_slash_height"
        android:text="/"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/admin_sales_list_et_dd_to"
        app:layout_constraintStart_toEndOf="@+id/admin_sales_list_et_dd_to"
        app:layout_constraintTop_toTopOf="@+id/admin_sales_list_et_dd_to" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="@dimen/admin_fragment_sales_history_slash_width"
        android:layout_height="@dimen/admin_fragment_sales_history_slash_height"
        android:text="/"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/admin_sales_list_et_yyyy_from"
        app:layout_constraintEnd_toStartOf="@+id/admin_sales_list_et_yyyy_from"
        app:layout_constraintTop_toTopOf="@+id/admin_sales_list_et_yyyy_from" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO"
        android:textSize="@dimen/admin_fragment_sales_history_date_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/admin_sales_list_btn_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toEndOf="@+id/admin_sales_list_et_yyyy_to"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/admin_sales_btn_export"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EXPORT"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/admin_sales_list_et_mm_from"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>